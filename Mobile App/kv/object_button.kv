<ObjectButton>:


<ObjectButton>:
    BoxLayout:
        top: root.top - 300
        orientation: "horizontal"
        width:root.width
        Button:
            text: "Coffee Cup"
            font_size: 15
            height: 20
            size_hint: .8,.8
            on_press: root.find_object(self)
            background_color: (1.0, 0.0, 0.0, 1.0)
        Button:
            text: "Water Bottle"
            font_size: 15
            size_hint: .8,.8
            on_press: root.find_object(self)
            background_color: (1.0, 0.0, 0.0, 1.0)
        Button:
            text: "Ball"
            font_size: 15
            size_hint: .8,.8
            on_press: root.find_object(self)
            background_color: (1.0, 0.0, 0.0, 1.0)
        Button:
            text: "Bowl"
            font_size: 15
            size_hint: .8,.8
            on_press: root.find_object(self)
            background_color: (1.0, 0.0, 0.0, 1.0)


